{
  "name": "lucario/skeleton",
  "type": "project",
  "description": "Lucario's skeleton to start developing websites",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "config/config.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
  },
  "require": {
    "lepelley/lucario": "^0",
    "nikic/fast-route": "^1.3",
    "twig/twig": "^3.0"
  },
  "require-dev": {
  },
  "repositories":[
    {
      "type": "package",
      "package": {
        "name": "lepelley/lucario",
        "version" : "0.1",
        "source": {
          "url": "https://github.com/Lepelley/Lucario.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "psr-4": {
            "Lucario\\": ["src/"]
          }
        }
      }
    }
  ]
}
